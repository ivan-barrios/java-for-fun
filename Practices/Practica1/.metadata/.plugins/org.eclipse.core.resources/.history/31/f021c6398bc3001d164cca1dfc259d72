package ejercicio6;

import java.util.Scanner;

public class Temperatura {
	
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Ingrese grados en Farenheit");
		double far = sc.nextDouble();
		System.out.println("Los grados en celcius es: " + calcularCelcius(far));
		
	}
	
	
	double calcularCelcius(double enFarenheit) {
		return ((32 - enFarenheit) * 5/9);
	}
}