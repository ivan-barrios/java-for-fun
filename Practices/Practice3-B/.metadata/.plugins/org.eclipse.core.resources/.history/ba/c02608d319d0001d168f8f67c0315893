package prog3.util;

public class PilaTest {

	public static void main(String[] args) {
		Pila<Character> pila = new Pila<Character>();
		pila.datos.comenzar();
		pila.apilar('a');
		pila.apilar('b');
		pila.apilar('c');
		pila.apilar('d');
		pila.apilar('e');
		pila.desapilar();
		pila.desapilar();
		pila.desapilar();
		pila.desapilar();
		System.out.println(pila.tope());
	}

}
