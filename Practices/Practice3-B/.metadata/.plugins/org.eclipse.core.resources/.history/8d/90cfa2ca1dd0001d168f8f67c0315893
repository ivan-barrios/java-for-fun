package prog3.util;

public class TestBalanceo {

	public static void main(String[] args) {
		String myString = "{()[()]}";
		System.out.println(verificarArmonia(myString));
	}

	public static boolean verificarArmonia(String myString) {
		Pila<Character> pila = new Pila<Character>();
		for (int i = 0; i < myString.length(); i++) {
			pila.apilar(myString.charAt(i));
		}
		boolean armonico = verificarRecursive(pila);
	}
	
	public static boolean verificarRecursive(Pila<Character> pila) {
		boolean armonico = false;
		
		
		
		return armonico;
	}

}
