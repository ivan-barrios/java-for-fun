package src;

public class EjercicioTestSobreescritura {

	public static void main(String[] args) {
		
		Tablet sp1 = new Tablet();
		Tablet sp2 = new Tablet();
		
		sp1.setPulgadas(9);
		sp1.setMarca("Apple");
		sp1.setCosto(1000);
		sp1.setSistemaOperativo(Window);
		sp2.setPulgadas(7);
		sp2.setMarca("Apple");
		
		boolean eq = sp1.equals(sp2);
		
		System.out.println(eq);
		
		System.out.println(sp1.toString());
	}

}
